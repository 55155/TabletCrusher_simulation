<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="Crusher" args="Revolute 10_position_controller Revolute 12_position_controller Revolute 13_position_controller Slider 21_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
